generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Referrals {
  id            Int      @id @default(autoincrement())
  referer_name  String
  referer_email String
  referee_name  String
  referee_email String
  Date          DateTime @default(now())
}
